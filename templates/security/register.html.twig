{% extends 'base.html.twig' %}

{% block navLinks %}
    {{ parent() }}
    <li><a href="{{ path('login') }}">Login</a></li>
{% endblock %}

{% block body %}
    {{ form_start(form) }}
    {{ form_row(form.email) }}
    {{ form_row(form.plainPassword.first) }}
    {{ form_row(form.plainPassword.second) }}

    <button type="submit">Register!</button>
    {{ form_end(form) }}
{% endblock %}